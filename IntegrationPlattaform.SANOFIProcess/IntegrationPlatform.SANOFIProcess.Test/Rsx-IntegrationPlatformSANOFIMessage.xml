<?xml version="1.0" encoding="utf-8" ?>
<Settings>
  <SanofiMessage>

    <!--SanofiLogo-->
    <key name="Company_DefaultLogoUrl">
      <![CDATA[https://s3.amazonaws.com/devproveedoresonline/MarketPlace/SiteImages/SANOFIDefaultLogo.jpg]]>
    </key>
    
    <!--SanofiMessageEmailTo-->
    <key name="POL_SanofiMessage_To">
      <![CDATA[david.moncayo@publicar.com]]>
    </key>

    <!--Fragment of provider info-->
    <key name="SanofiFileMessage">
      <![CDATA[
          <tr style="height: 36px;">
            <td>
              <strong>{InformationType}</strong>
            </td>
          </tr>
          <tr style="height: 30px;">
            <td>
              <a target="_blank" style="color: #0066ae; font-size: 1em; letter-spacing: -0.8px; font-weight:bold;" href="{InfoFileUrl}" title="Descargar archivo">Descargar archivo</a>
            </td>
            <td>
              <span>fecha de generación: {FileCreateDate}</span>
            </td>
          </tr>
      ]]>
    </key>
    
  </SanofiMessage>
</Settings>