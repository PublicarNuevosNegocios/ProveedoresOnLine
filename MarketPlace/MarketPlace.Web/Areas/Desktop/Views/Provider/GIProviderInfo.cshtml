                                        </div>
                                        <div class="col-xs-12">
                                            @{
                                    string FK_TotalKValueScore = Convert.ToDecimal(item.FK_TotalKValueScore).ToString("#,0.##", System.Globalization.CultureInfo.CreateSpecificCulture("ES-co"));
                                            }
