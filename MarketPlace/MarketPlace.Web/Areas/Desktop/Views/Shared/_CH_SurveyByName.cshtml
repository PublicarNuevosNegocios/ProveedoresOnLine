<div id="dashboard_div_nm">
    <div id="filter_year_nm"></div>
    <div id="SurveyByNameChartId" class="ResponsiveChartsClass"></div>
</div>

<script type="text/javascript">
    google.load('visualization', '1.0', { 'packages': ['controls'] });
    $(document).ready(function () {

        google.setOnLoadCallback(SurveyByName_ChartsObject.RenderChart);

        SurveyByName_ChartsObject.Init({
            ObjectId: 'SurveyByNameChartId',
            DashboardId: 'dashboard_div_nm',
            SurveyResponsible: 'true',

            SearchUrl: '@(Url.RouteUrl(MarketPlace.Models.General.Constants.C_Routes_Default,
                           new
                            {
                               controller = MVC.Provider.Name,
                               action = MVC.Provider.ActionNames.Search
                            }))',
        })
    })
</script>
