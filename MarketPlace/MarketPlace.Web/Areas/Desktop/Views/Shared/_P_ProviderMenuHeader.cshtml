@model MarketPlace.Models.Provider.ProviderViewModel

@if (Model.RenderScripts)
{
    <img src="@{Model.RelatedLiteProvider.Where(x => x.ProviderLogoUrl != null).Select(x => x.ProviderLogoUrl).FirstOrDefault();}"
         alt="@{Model.RelatedLiteProvider.Where(x => x.RelatedProvider != null).Select(x => x.RelatedProvider.RelatedCompany.CompanyName).FirstOrDefault();}" />

    if (Model.IsOwnProvider)
    {
        <img src="@{Model.RelatedLiteProvider.Where(x => x.ProviderRate != null).Select(x => x.ProviderRate).FirstOrDefault();}" />
    }
}
else
{

}