<div id="dashboard_div">
    <div id="filter_div_year"></div>
    <div id="SurveyResponsableStatusChartId" class="ResponsiveChartsClass"></div>
</div>

@{
    var email = 0;

    if (MarketPlace.Models.General.SessionModel.CurrentCompany.RelatedUser.FirstOrDefault().RelatedRole.ParentItem != null)
    {
        // NORMAL USER
        email = MarketPlace.Models.General.SessionModel.CurrentLoginUser.RelatedApplicationRole.Where(x => x.ApplicationRoleId != null).Select(x => x.ApplicationRoleId).DefaultIfEmpty(0).FirstOrDefault();
    }
}

<script type="text/javascript">
    google.load('visualization', '1.0', { 'packages': ['controls'] });
    $(document).ready(function () {

        google.setOnLoadCallback(Survey_ChartsObject.RenderChartSurveyByResponsable);

        Survey_ChartsObject.Init({
            ObjectId: 'SurveyResponsableStatusChartId',
            DashboardId: 'dashboard_div',
            SurveyResponsible: 'true',
            UserEmail: '@(email)',
            SearchUrl: '@(Url.RouteUrl(MarketPlace.Models.General.Constants.C_Routes_Default,
                           new
                            {
                               controller = MVC.Provider.Name,
                               action = MVC.Provider.ActionNames.Search
                            }))',
        })
    })
</script>

