<?xml version="1.0" encoding="utf-8" ?>
<Settings>
  <BackOffice>
    <!--Login Module-->
    <key name="Login_GoogleUrl">https://localhost:44305/GoogleLogin/Login?UrlRetorno={{UrlRetorno}}</key>

    <key name="Login_UserNotAutorized">El usuario {UserName} no esta autorizado para ingresar a la plataforma.</key>

    <!--Generic params-->
    <key name="Grid_RowCountDefault">20</key>

    <key name="Path_Salesforce">https://na2.salesforce.com/</key>

    <key name="DateFormat_Kendo"><![CDATA[{0:yyyy-MM-dd}]]></key>
    <key name="DateFormat_KendoToServer"><![CDATA[yyyy-MM-ddTHH:mm:ss.fffZ]]></key>
    <key name="DateFormat_Server"><![CDATA[yyyy-MM-dd]]></key>
    
    <key name="CurrencyExchange_USD">108001</key>

    <!--Administrator User Roles-->
    <key name="URL_AdminRoles">https://localhost:44305/AdminRoles/Index</key>

    <!--Document Managment-->
    <key name="URL_DocumentManagment">https://localhost:44306/</key>

    <!--File Params-->
    <key name="File_TempDirectory">/TempDirectory/</key>
    <key name="File_RemoteDirectory">\BackOffice\</key>

  </BackOffice>
</Settings>

