@model BackOffice.Models.Provider.ProviderViewModel

@{
    ViewBag.Title = "Validar Proveedores";
    Layout = "~/Views/Shared/_Layout.cshtml";

    //Model.RenderScripts = false;
}
@*ESTE ES EL ENVUELTO COMPLETO DE MENU LATERAL Y CONTENIDO*@
<div class="POBOProviderWrapper">
    @*ESTE ES QUE TRAE EL PARCIAL QUE PINTA EL MENU LATERAL*@
    <div class="POBOProviderSideMenuWrapper">
        @Html.Partial(MVC.Shared.Views._P_ProviderMenu, Model)
    </div>
    <div class="POBOProviderContentWrapper">
        <form action="@Url.Action(MVC.Admin.ActionNames.AdminRLUploadProvider, MVC.Admin.Name)" method="post" enctype="multipart/form-data">
            <ul>
                <li>
                    <span>Archivo a cargar:</span>
                    <input type="file" name="ExcelFile" id="ExcelFile" />
                </li>
                <li><input class="BOButtons" type="submit" value="Validar en World Check" /></li>
            </ul>
        </form>
    </div>
</div>

@{
    Model.RenderScripts = true;
}

@section inlinescripts{
    @Html.Partial(MVC.Shared.Views._P_ProviderMenu, Model);
}

