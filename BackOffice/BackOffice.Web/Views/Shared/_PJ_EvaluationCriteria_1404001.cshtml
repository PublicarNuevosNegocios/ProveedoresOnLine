@model BackOffice.Models.Customer.EvaluationItemViewModel

<div>
    <label>Criterio de Evaluación de Experiencias</label>

    @if (Model != null)
    {
        <ul>
            <li>
                <label>Unidad:</label>
                <input id="@Model.Where(x => x.EC_UnitId != null).Select(x => x.EC_UnitId).FirstOrDefault()" name="EC_UnitId" value="@Model.Where(x => x.EC_Unit != null).Select(x => x.EC_Unit).FirstOrDefault()" />
            </li>
            <li>
                <label>Criterio de Evaluación:</label>
                <input id="@Model.Where(x => x.EC_EvaluationCriteriaId != null).Select(x => x.EC_EvaluationCriteriaId).FirstOrDefault()" name="EC_EvaluationCriteriaId" value="@Model.Where(x => x.EC_EvaluationCriteria != null).Select(x => x.EC_EvaluationCriteria).FirstOrDefault()" />
            </li>
            <li>
                <label>Orden</label>
                <input id="@Model.Where(x => x.EC_OrderId != null).Select(x => x.EC_OrderId).FirstOrDefault()" name="EC_OrderId" value="@Model.Where(x => x.EC_Order != null).Select(x => x.EC_Order).FirstOrDefault()" />
            </li>
            <li>
                <label>Habilitado:</label>
                <input type="checkbox" checked />
                <label>Peso:</label>
                <input id="@Model.Where(x => x.EC_RatingId != null).Select(x => x.EC_RatingId).FirstOrDefault()" name="EC_RatingId" value="@Model.Where(x => x.EC_Rating != null).Select(x => x.EC_Rating).FirstOrDefault()" />
            </li>
            <li>
                <label>Habilitado:</label>
                <input type="checkbox" checked />
                <label>Configuración de Experiencia</label>
                <div>
                    <ul>
                        <li>
                            <label>Años de experiencia a tener en cuenta</label>
                            <div>
                                <ul>
                                    <li>
                                        <label>Tipo de Fecha:</label>
                                        <input />
                                    </li>
                                    <li>
                                        <label>Identificador:</label>
                                        <input />
                                    </li>
                                    <li>
                                        <label>Operador:</label>
                                        <input />
                                    </li>
                                    <li>
                                        <label>Etiqueta:</label>
                                        <input />
                                    </li>
                                </ul>
                            </div>
                        </li>
                        <li>
                            <label>Cantidad minima de experiencias:</label>
                            <div>
                                <ul>
                                    <li>
                                        <label>Identificador:</label>
                                        <input />
                                    </li>
                                    <li>
                                        <label>Etiqueta:</label>
                                        <input />
                                    </li>
                                    <li>
                                        <label>Habilitado</label>
                                        <input type="checkbox" checked />
                                    </li>
                                </ul>
                            </div>
                        </li>
                        <li>
                            <label>Actividades Económicas:</label>
                            <input type="checkbox" checked />
                        </li>
                        <li>
                            <label>Actividades Económicas Personalizadas:</label>
                            <input type="checkbox" checked />
                        </li>
                        <li>
                            <label>Valor estimado:</label>
                            <input type="checkbox" checked />
                        </li>
                        <li>
                            <label>Moneda:</label>
                            <input type="checkbox" checked />
                        </li>
                    </ul>
                </div>
            </li>
        </ul>
    }
    else
    {
        <ul>
            <li>
                <label>Unidad:</label>
                <input name="EC_UnitId" value="" />
            </li>
            <li>
                <label>Criterio de Evaluación:</label>
                <input name="EC_EvaluationCriteriaId" value="" />
            </li>
            <li>
                <label>Orden</label>
                <input name="EC_OrderId" value="" />
            </li>
            <li>
                <label>Habilitado:</label>
                <input type="checkbox" checked />
                <label>Peso:</label>
                <input name="EC_RatingId" value="" />
            </li>
            <li>
                <label>Habilitado:</label>
                <input type="checkbox" checked />
                <label>Configuración de Experiencia</label>
                <div>
                    <ul>
                        <li>
                            <label>Años de experiencia a tener en cuenta</label>
                            <div>
                                <ul>
                                    <li>
                                        <label>Tipo de Fecha:</label>
                                        <input />
                                    </li>
                                    <li>
                                        <label>Identificador:</label>
                                        <input />
                                    </li>
                                    <li>
                                        <label>Operador:</label>
                                        <input />
                                    </li>
                                    <li>
                                        <label>Etiqueta:</label>
                                        <input />
                                    </li>
                                </ul>
                            </div>
                        </li>
                        <li>
                            <label>Cantidad minima de experiencias:</label>
                            <div>
                                <ul>
                                    <li>
                                        <label>Identificador:</label>
                                        <input />
                                    </li>
                                    <li>
                                        <label>Etiqueta:</label>
                                        <input />
                                    </li>
                                    <li>
                                        <label>Habilitado</label>
                                        <input type="checkbox" checked />
                                    </li>
                                </ul>
                            </div>
                        </li>
                        <li>
                            <label>Actividades Económicas:</label>
                            <input type="checkbox" checked />
                        </li>
                        <li>
                            <label>Actividades Económicas Personalizadas:</label>
                            <input type="checkbox" checked />
                        </li>
                        <li>
                            <label>Valor estimado:</label>
                            <input type="checkbox" checked />
                        </li>
                        <li>
                            <label>Moneda:</label>
                            <input type="checkbox" checked />
                        </li>
                    </ul>
                </div>
            </li>
        </ul>
    }
</div>