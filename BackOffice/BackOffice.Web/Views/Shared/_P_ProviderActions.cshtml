@model BackOffice.Models.Provider.ProviderViewModel

@if (!Model.RenderScripts)
{
    <ul>
        @if (Model.ProviderMenu != null &&
            Model.ProviderMenu.Count > 0)
        {
            if (Model.CurrentSubMenu != null &&
                Model.CurrentSubMenu.LastMenu != null)
            {
                <li>
                    <a href="javascript:Provider_SubmitForm({FormId:'frmProviderUpsert',StepValue:'last'})">Anterior</a>
                </li>
            }
            if (Model.CurrentSubMenu != null &&
                Model.CurrentSubMenu.NextMenu != null)
            {
                <li>
                    <a href="javascript:Provider_SubmitForm({FormId:'frmProviderUpsert',StepValue:'next'})">Siguiente</a>
                </li>
            }
        }
        <li>
            <a href="javascript:Provider_SubmitForm({FormId:'frmProviderUpsert'})">Guardar</a>
        </li>
    </ul>
}
